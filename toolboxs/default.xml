<xml>
  <externals>
    <module src="blocks/test.js"></module>
  </externals>
  <category name="%{BKY_CATEGORY_SIGNALS}" colour="%{BKY_SIGNALS_HUE}">
    <block type="signal_do"></block>
    <block type="signal_new_sprite_as_receiver"></block>
    <block type="signal_when_receiver_is"></block>
    <block type="signal_onevent_send">
      <value name="SPRITE">
        <block type="sprite"></block>
      </value>   
    </block>
    <block type="get_data"></block>
    <block type="get_data_prop"></block>
    <block type="get_store_data_prop"></block>
  </category>
  <category name="%{BKY_CATEGORY_PROCEDURE}" colour="%{BKY_PROCEDURE_HUE}">
    <block type="procedures_defreturn">
      <field name="NAME">myfunc</field>
    </block>
    <block type="procedures_ifreturn"></block>
    <block type="procedures_await"></block>
    <block type="procedures_call"></block>
  </category>
  <category name="%{BKY_CATEGORY_SPRITES}" colour="%{BKY_SPRITE_HUE}">
